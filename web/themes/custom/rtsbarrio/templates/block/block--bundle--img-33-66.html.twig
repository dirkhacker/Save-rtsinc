{% set img_url = content.field_img_33_66__img[0]['#media'].field_media_image.entity.uri.value %}


<section class="img-33-66 my-5">
    <div class="container">
        <div class="d-flex">
            <img width="342" height="487" class="me-5" src="{{ img_url|file_url }}" alt="" />
