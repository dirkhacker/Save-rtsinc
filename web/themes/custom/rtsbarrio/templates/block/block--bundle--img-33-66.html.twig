                            <p class="card-text">{{ content.field_img_33_66__card_body2[0]['#context'].value }}</p>
                        </div>
                    </div>
                </div>
                {% if content.field_img_33_66__card_header3 or content.field_img_33_66__card_header4 is not empty %}
